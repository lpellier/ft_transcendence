{
	"compilerOptions": {
	  "target": "es6",
	  "lib": [
		"dom",
		"dom.iterable",
		"esnext"
	  ],
	  "allowJs": true,
	  "skipLibCheck": true,
	  "esModuleInterop": true,
	  "allowSyntheticDefaultImports": true,
	  "strict": true,
	  "forceConsistentCasingInFileNames": true,
	  "noFallthroughCasesInSwitch": true,
	  "module": "esnext",
	  "moduleResolution": "node",
	  "resolveJsonModule": true,
	  "isolatedModules": false,
	  "noEmit": true,
	  "jsx": "react-jsx",
	  "baseUrl": "./src",
      "paths": {
          "@component/*": ["components/*"],
          "@images/*": ["images/*"],
		  "@tsxStyles/*": ["styles/tsxStyles/*"]
	  }
	},
	"include": [
	  "src"
	],
	"exclude": [
		"src/components/Game",
		"src/p5",
		"src/pages/Gamepage/Game",
		"src/pages/Gamepage/p5"
	]
  }